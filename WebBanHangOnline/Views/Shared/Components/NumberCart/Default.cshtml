@model List<WebBanHangOnline.ViewModels.CartItem>
@if (Model != null  && Model.Count() > 0 )
{
   
        <a asp-controller="ShoppingCart" asp-action="Index"><i class="fa fa-shopping-bag"></i> <span>@Model.Count()</span>
            </a>
    
}else
{
    
        <a asp-controller="ShoppingCart" asp-action="Index"><i  class="fa fa-shopping-bag"></i> <span>0</span>
        
    </a>
   
}